{
  "private": true,
  "name": "pix-tutos",
  "version": "2.2.1",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate && npm run signal-jira",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "signal-jira": "node ./scripts/jira/comment-with-review-app-url.js",
    "test": "vitest",
    "test:e2e": "npm run test:e2e:install && npm run test:e2e:run",
    "test:e2e:install": "playwright install",
    "test:e2e:run": "playwright test",
    "postinstall": "nuxt prepare"
  },
  "engines": {
    "node": "18",
    "npm": "<10"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.14",
    "@fontsource/roboto": "^4.5.8"
  },
  "devDependencies": {
    "@nuxt/content": "^2.4.3",
    "@nuxt/eslint-config": "^0.1.1",
    "@playwright/test": "^1.30.0",
    "@types/node": "^18.14.0",
    "axios": "^1.3.3",
    "eslint": "^8.40.0",
    "nuxt": "~3.5.0",
    "sass": "^1.52.3",
    "vitest": "^0.31.0"
  }
}
