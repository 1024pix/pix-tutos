{
  "private": true,
  "name": "pix-tutos",
  "version": "1.3.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate && npm run signal-jira",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "signal-jira": "node ./scripts/jira/comment-with-review-app-url.js",
    "test": "vitest",
    "test:e2e": "npm run test:e2e:install && npm run test:e2e:run",
    "test:e2e:install": "playwright install",
    "test:e2e:run": "playwright test",
    "postinstall": "nuxt prepare"
  },
  "engines": {
    "node": "16",
    "npm": "<10"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.10",
    "@fontsource/roboto": "^4.5.7"
  },
  "devDependencies": {
    "@nuxt/content": "^2.4.3",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.0.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@playwright/test": "^1.30.0",
    "@types/node": "^18.14.0",
    "axios": "^1.3.3",
    "eslint": "^8.40.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "~3.3.0",
    "prettier": "^2.8.4",
    "sass": "^1.52.3",
    "vitest": "^0.28.5"
  }
}
